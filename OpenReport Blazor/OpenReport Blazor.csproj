<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>OpenReport Blazor</RootNamespace>
    <AssemblyName>$(AssemblyName.Replace(' ', '_'))</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FastReport.Compat" Version="*" />
    <PackageReference Include="FastReport.OpenSource" Version="*" />
    <PackageReference Include="FastReport.OpenSource.Web" Version="*" />
    <PackageReference Include="FastReport.OpenSource.Export.PdfSimple" Version="*" />
  </ItemGroup>

</Project>
